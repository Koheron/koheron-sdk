/dts-v1/;
#include "zynq-7000.dtsi"
/ {
	model = "Koheron Alpha250";
	compatible = "xlnx,zynq-alpha250","xlnx,zynq-7000";
	aliases {
		serial0 = &uart0;
        mmc0 = &sdhci0;
	};
	memory {
		device_type = "memory";
		reg = <0 0x1E000000>;
	};
	chosen {
		bootargs = "";
		stdout-path = "serial0:115200n8";
	};
	usb_phy0: phy0 {
		compatible = "usb-nop-xceiv";
		#phy-cells = <0>;
	};
};
&sdhci0 {
	u-boot,dm-pre-reloc;
	status = "okay";
};
&uart0 {
	u-boot,dm-pre-reloc;
	status = "okay";
};