&{/} {
	koheron_mem: koheron-mem {
		compatible = "koheron,memory-1.0";
		#address-cells = <1>;
		#size-cells   = <1>;
		ranges;

{%- for r in regions %}
		{{r.name}}: {{r.name}}@{{"%08x"|format(r.base)}} {
			koheron_name = "{{r.name}}";
			reg = <0x{{"%08x"|format(r.base)}} 0x{{"%08x"|format(r.size)}}>;
		};
{%- endfor %}
	};
};